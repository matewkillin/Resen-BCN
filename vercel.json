{
    "version": 2,
    "builds": [
      {
        "src": "index.html",
        "use": "@vercel/static-build"
      },
      {
        "src": "telefono.html",
        "use": "@vercel/static-build"
      },
      {
        "src": "styles.css",
        "use": "@vercel/static-build"
      },
      {
        "src": "script.js",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  